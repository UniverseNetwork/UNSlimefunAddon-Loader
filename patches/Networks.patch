+                .addFlavourProfile(0, 0, 5, 0, 0)
+                .tryRegister(SlimefunAddonInstance.getSFAInstance());
+
+        new HarvestableSeed(SYNTHETIC_EMERALD_SEED)
+                .setHarvestingResult(SlimefunItems.SYNTHETIC_EMERALD)
